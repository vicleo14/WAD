  	<servlet-class>mx.ipn.escom.web.servlets.Home</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FormLogIn</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletAutenticacion</servlet-name>
  	<url-pattern>/ServletAutenticacion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
